{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES2022",
    "module": "CommonJS",
    "baseUrl": ".",
    "outDir": "./dist",
    "esModuleInterop": true,
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@layouts/*": ["src/layouts/*"],
      "@modules/*": ["src/modules/*"],
      "@typings/*": ["src/typings/*"],
      "@data/*": ["src/data/*"]
    }
  }
}
